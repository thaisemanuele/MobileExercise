package com.app.jobapplication.factory;

import java.util.ArrayList;
import java.util.List;

import com.app.jobapplication.models.vo.Transaction;

public class TransactionFactory {
	
	public static List<Transaction> getList(){
		List<Transaction> transactions = new ArrayList<Transaction>();
		transactions.add(new Transaction("Apple Store","R$","2600,00",false));
		transactions.add(new Transaction("Forever 21","R$","200,00",false));
		transactions.add(new Transaction("Primark","£","260,00",false));
		transactions.add(new Transaction("Apple Store","$","600,00",false));
		transactions.add(new Transaction("C&A","R$","160,00",false));
		transactions.add(new Transaction("Marks & Spencer","£","500,00",false));
		transactions.add(new Transaction("Lilly Adams","R$","1600,00",false));
		return transactions;
	}

}
