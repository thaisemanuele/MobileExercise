package com.app.jobapplication.models.vo;

/**
 * Class that represents a transaction
 * example: 
 * Apple Store (place); R$(currency), 2600,00(value),
 * 		false(chargeback was not submitted)
 * @author Thais
 *
 */
public class Transaction {
	
	private String place;
	private String currency;
	private String value;
	private Boolean chargeback;
	public String getTitle() {
		return title;
	}
	public void setTitle(String title) {
		this.title = title;
	}
	public String getCurrency() {
		return currency;
	}
	public void setCurrency(String currency) {
		this.currency = currency;
	}
	public String getValue() {
		return value;
	}
	public void setValue(String value) {
		this.value = value;
	}
	public Boolean getChargeback() {
		return chargeback;
	}
	public void setChargeback(Boolean chargeback) {
		this.chargeback = chargeback;
	}
}
